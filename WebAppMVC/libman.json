{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/css/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.css",
        "css/bootstrap.min.css",
        "js/bootstrap.js"
